{
	"@layout":[
        {
            "@isa":"UIView",
            "@id":"bkg",
            "@style":"defaultCell",
            "frame":[0,0,"@metric(#{appearance.screenWidth})",44],
            "autoresizing-mask":["width"]
		},
		{
			"@isa":"LTLabel",
			"@id":"l1",
			"@style":"defaultLabel",
			"text":"Ciao: #{@bind(object.status)} ",
			"frame":[10,0, "@metric(#{appearance.screenWidth}-100)","@metric(#{bkg.frameHeight})"]
        },
        {
            "@isa":"UIButton",
            "@id":"coolButton",
            "@style":"embossedButton",
            "title":"@icon(github)",
            "frame":["@metric(#{l1.frameWidth}+25)",5,60,"@metric(#{bkg.frameHeight}-10)"]
        }
	]
    
    
}
			  
